<%= content_tag :div, id: "image-#{image.id}", :class => "manual-image " + image.feature_image_class(image.id) do %>

  <%= image_tag image.file.url(:thumb) %>
  <span class="featured">Cover image</span>
  <%= link_to "Set as cover image", set_feature_path(image.id), :method => :put, :class => "set-feature" %>
  <%= link_to "x", image_path(image), :method => :delete, :class => "delete-image", :confirm => "Delete image?" %>

<% end %>